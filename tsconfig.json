{
  "compilerOptions": {
      "module": "commonjs",
      "strict": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "declaration": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "target": "ESNext",
      "alwaysStrict": true,
      "incremental": true,
      "noImplicitAny": true,
      "sourceMap": true
  }
}